{
  "name": "@calcit/editor",
  "version": "0.8.1",
  "description": "Tree Editor for Calcit Language",
  "bin": {
    "ct": "./server.mjs"
  },
  "scripts": {
    "watch-server": "cr --emit-js",
    "compile-server": "cr --emit-js --once",
    "compile-page": "cr --emit-js --once --entry client --emit-path out-page",
    "release-page": "vite build --base=./",
    "watch-page": "cr --emit-js --emit-path out-page/ --entry client"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:calcit-lang/editor.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "bottom-tip": "^0.1.5",
    "feather-icons": "^4.29.1",
    "url-parse": "^1.5.10",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@calcit/procs": "^0.8.0-a2",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.9",
    "gaze": "^1.1.3",
    "latest-version": "^7.0.0",
    "md5": "^2.3.0",
    "nanoid": "^4.0.2",
    "ws": "^8.13.0"
  }
}
